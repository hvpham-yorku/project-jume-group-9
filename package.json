{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "supabase:start": "supabase start && npm run db:reset",
    "supabase:stop": "supabase stop",
    "supabase:reset": "npm run supabase:stop && npm run supabase:start",
    "supabase:db:reset": "supabase db reset",
    "supabase:typegen:local": "supabase gen types typescript --local > utils/supabase/types/db.ts",
    "docker:clean": "docker stop $(docker ps -q)",
    "seed:sync": "npx @snaplet/seed sync",
    "seed:gen": "npm run seed:sync && npx tsx --env-file=.env mock/index.ts > supabase/seeds/seed.sql",
    "seed:img": "npx tsx --env-file=.env mock/images.ts",
    "seed:delete": "rimraf supabase/seeds/seed.sql",
    "db:reset": "npm run seed:gen && npm run supabase:db:reset && npm run seed:img && npm run supabase:typegen:local"
  },
